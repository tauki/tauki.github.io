---
layout: null
---
[
  {% assign prose_list = site.prose_categories %}
  {% assign all_notes = site.notes | sort: 'date' | reverse %}
  {% assign prose_notes = "" | split: "" %}
  
  {% for note in all_notes %}
    {% assign is_prose = false %}
    {% for cat in note.categories %}
        {% if prose_list contains cat %}
            {% assign is_prose = true %}
            {% break %}
        {% endif %}
    {% endfor %}
    
    {% if is_prose %}
        {% assign prose_notes = prose_notes | push: note %}
    {% endif %}
  {% endfor %}

  {% for note in prose_notes %}
    {
      "title": {{ note.title | jsonify }},
      "slug": {{ note.title | slugify | jsonify }},
      "url": {{ note.url | relative_url | jsonify }},
      "date": {{ note.date | jsonify }},
      "subtitle": {{ note.subtitle | jsonify }},
      "categories": {{ note.categories | jsonify }},
      "content": {{ note.content | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
